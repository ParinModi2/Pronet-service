create database IF NOT EXISTS pronet;
use pronet;

drop table SignIn;
create table SignIn
(
ID int NOT NULL AUTO_INCREMENT,
name varchar(255),
email varchar(50),
password varchar(25),
role varchar(2),
PRIMARY KEY (ID)
);
commit;
select * from SignIn;

desc SignIn;

//create follow table
drop table follow;
create table follow
(
followerID varchar(255),
followeeID varchar(255),
followeeImgURL varchar(255),
followeeName varchar(255),
followeeRole varchar(255)
);
commit;
select * from follow;
